- var site =  imported.site 

footer
  .container
    .row
      .col-md-4
        span.copyright Copyright &copy; #{ site.title } #{ site.copyRightYear }
      .col-md-4
        ul.list-inline.social-buttons
          each network,i in site.social
            li
              a(href= network.url )
                i.fa(class= network.title)

      .col-md-4
        ul.list-inline.quicklinks
          li
            a(href='#') Privacy Policy
          li
            a(href='#') Terms of Use
